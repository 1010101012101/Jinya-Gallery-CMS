{% extends '@Backend/base.html.twig' %}

{% set menu_access_log_active = true %}
{% block content %}
    <div class="table-responsive">
        <table class="table table-hover table-striped" data-tables="true" data-sort-by="created_at"
               data-sort-order="desc" data-service-url="{{ url('backend_accesslog_getlist') }}" cellspacing="0">
            <thead>
            <tr>
                <th data-name="ip" data-data="ip">{{ 'backend.accesslog.ip'|trans }}</th>
                <th data-name="uri" data-data="uri">{{ 'backend.accesslog.uri'|trans }}</th>
                <th data-name="method" data-data="method">{{ 'backend.accesslog.method'|trans }}</th>
                <th data-name="created_at" data-data="createdAt"
                    data-format="dateTimeFormat">{{ 'backend.accesslog.created_at'|trans }}</th>
            </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
{% endblock %}
{% block sidebar_items %}
    {% include 'BackendBundle:sidebar:log.html.twig' %}
{% endblock %}