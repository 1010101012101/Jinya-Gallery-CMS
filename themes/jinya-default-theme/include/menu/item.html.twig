{%- block menuitem -%}
    <li class="jinya-menu__item {% if isChildActiveMenuItem(item) %}jinya-menu__item--active{% endif %} {% if item.highlighted %}jinya-menu__item--highlight{% endif %}">
        {% if item.children is not empty %}
            {% for child in item.children %}
                {% include '@Themes/jenny-jinya-theme/include/submenu/submenu.html.twig' with {'item': child} %}
            {% endfor %}
        {% else %}
            {% include '@Themes/jenny-jinya-theme/include/menu_link.html.twig' with {'item': child, 'class': 'jinya-menu__link'} %}
        {% endif %}
    </li>
{%- endblock -%}