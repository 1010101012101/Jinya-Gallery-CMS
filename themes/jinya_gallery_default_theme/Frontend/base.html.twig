<html>
<head>
    <title>{{ themeConfig.page.title }} {% block head_title %}{% endblock %}</title>
    {% block styles %}
        {% block styles_theme %}
            <link rel="stylesheet"
                  href="{{ asset('public/jinya_gallery_default_theme/styles/all.css') }}">
        {% endblock %}
        {% block styles_custom %}
        {% endblock %}
    {% endblock %}
    {% block favicon %}
        <link href="{{ themeConfig.page.favicon }}" rel="shortcut icon">
    {% endblock %}
    {% block meta %}
        <meta charset="utf-8"/>
        <meta http-equiv="X-UA-Compatible" content="IE=Edge"/>
        <meta name="viewport" content="width=device-width"/>
        {% block custom_meta %}
        {% endblock %}
    {% endblock %}
    <noscript>
        <style>
            [data-simplebar] {
                overflow: auto;
            }
        </style>
    </noscript>
</head>
<body>
{% block body %}
    {% block background %}
    {% endblock %}
    {% block menu %}
        {% include '@Theme/jinya_gallery_default_theme/Frontend/include/menu.html.twig' %}
    {% endblock %}
    {% block maincontent %}
    {% endblock %}
    {% block shop_logo %}
        {% include '@Theme/jinya_gallery_default_theme/Frontend/include/shop_logo.html.twig' %}
    {% endblock %}
{% endblock %}
{% block scripts %}
{% block polyfills %}
    <script src="https://cdnjs.cloudflare.com/ajax/libs/iamdustan-smoothscroll/0.4.0/smoothscroll.min.js"
            integrity="sha256-IylDNWjip7FK2TJUYck237qBTBeSiwnSFDCjLG686D4=" crossorigin="anonymous"></script>
{% endblock %}
{% block scripts_theme %}
    <script src="{{ asset('public/jinya_gallery_default_theme/scripts/scripts.js') }}"></script>
{% endblock %}
{% block scripts_classie %}
    <script src="https://cdnjs.cloudflare.com/ajax/libs/classie/1.0.1/classie.min.js"
            integrity="sha256-6O5DljdGA6AiaEVC/Zn8pNLNbQGparYTI9091fGX64k=" crossorigin="anonymous"></script>
{% endblock %}
    {% block scripts_custom %}
    {% endblock %}
{% endblock %}
</body>
</html>
