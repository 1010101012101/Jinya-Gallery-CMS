    "incenteev/composer-parameter-handler": "^2.0",
    "leafo/scssphp": "^0.7.1",
    "ocramius/proxy-manager": "^2.1",
    "patchwork/jsqueeze": "^2.0",
    "sensio/framework-extra-bundle": "^5.1",
    "symfony/asset": "^4.0",
    "symfony/console": "^4.0",
    "symfony/debug-pack": "*",
    "symfony/dotenv": "^4.0",
    "symfony/expression-language": "^4.0",
    "symfony/flex": "^1.0",
    "symfony/form": "^4.0",
    "symfony/framework-bundle": "^4.0",
