{% if not ajax %}
<html>
<head>
    <title>{{ 'app.name'|trans() }}{% block title %}{% endblock %}</title>
    {% block styles %}{% endblock %}
    {% block meta %}
        <meta charset="utf-8"/>
        <meta http-equiv="X-UA-Compatible" content="IE=Edge"/>
        <meta name="viewport" content="width=device-width"/>
        {% block custom_meta %}
        {% endblock %}
    {% endblock %}
</head>
<body>
{% endif %}
{% block body %}{% endblock %}
{% if not ajax %}
{% block scripts %}{% endblock %}
</body>
</html>
{% endif %}
